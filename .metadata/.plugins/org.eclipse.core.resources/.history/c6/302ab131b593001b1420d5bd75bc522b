#include <run_cpp_utests.hpp>

#ifdef __cplusplus
	extern "C"
	{
#endif

	void run_cpputests()
	{
		const char * av_override[] = { };
		CommandLineTestRunner::RunAllTests(0, av_override);
	}



#ifdef __cplusplus
	}
#endif

